<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.BlowFreeApp"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="19"/>
    <application android:label="@string/app_name" android:icon="@drawable/ic_launcher">
        <activity android:name=".activities.MainMenu"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.PackageSelect" android:label="BlowFree"></activity>
        <activity android:name=".activities.Settings" android:label="BlowFree"></activity>
        <activity android:name=".activities.NewGame" android:label="BlowFree"></activity>
        <activity android:name=".activities.Game" android:label="BlowFree"></activity>
        <activity android:name=".activities.GameInstances" android:label="BlowFree"></activity>
        <activity android:name=".activities.DifficultySelector" android:label="BlowFree"></activity>
        <activity android:name=".activities.LevelSelectorEasy" android:label="BlowFree"></activity>
        <activity android:name=".activities.LevelSelectorMedium" android:label="BlowFree"></activity>
        <activity android:name=".activities.LevelSelectorHard" android:label="BlowFree"></activity>
    </application>
    <uses-permission android:name="android.permission.VIBRATE" />
</manifest>
